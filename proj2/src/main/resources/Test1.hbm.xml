<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name = "com.revature.driver.TestBean" table = "TEST">
		<id name = "tid" type = "int">
		<column name="TID" not-null="true" unique="true" length = "10"></column>
		<generator class="increment"></generator>
		</id>
		
		<property name="sid" type = "int">
		<column name="SID" length="10"></column>
		</property>
		
		<property name="name" type = "string">
		<column name="NAME" length="20"></column>
		</property>
		<property name="error" type = "string">
		<column name="ERROR" length="150"></column>
		</property>
		<property name="status" type = "string">
		<column name="STATUS" length="10"></column>
		</property>
		
	</class>
</hibernate-mapping>